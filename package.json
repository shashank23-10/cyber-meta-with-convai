{
  "name": "react-three-vite",
  "version": "1.0.2",
  "description": "A simple React Three Fiber + Vite starter with an import-ready CodeSandbox config.",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "sandbox": "APP_ENV=sandbox vite",
